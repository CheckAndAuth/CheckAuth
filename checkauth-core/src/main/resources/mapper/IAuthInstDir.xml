<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.check.auth.g3.checkauth.dao.IAuthInstDirDao">
    <select id="queryByInstCode"  resultType="authInstDirEntity" parameterType="java.lang.String">
        SELECT * FROM auth_org_view WHERE instCode=#{instCode}
    </select>

    <insert id="insertAuthInstDir" parameterType="java.util.List">
        insert into auth_org_view (id,instType,instTypeOrder,ratifyDateOrder,mainInstId,instCode,instName,instStatus,address,districtCode,districtCodeName,principal,ratifyDate,validityDate,contactPeroson,tel,cccBm,cccName) values
        <foreach collection="list" item="item" index="index" separator="," >
	        (#{item.id},#{item.instType},#{item.instTypeOrder},#{item.ratifyDateOrder},#{item.mainInstId},#{item.instCode},#{item.instName},#{item.instStatus},#{item.address},#{item.districtCode},#{item.districtCodeName},#{item.principal},#{item.ratifyDate},#{item.validityDate},#{item.contactPeroson},#{item.tel},#{item.cccBm},#{item.cccName})
        </foreach>
    </insert>
    
    <insert id="insert" parameterType="authInstDirEntity">
    	insert into auth_org_view (id,instType,instTypeOrder,ratifyDateOrder,mainInstId,instCode,instName,instStatus,address,districtCode,districtCodeName,principal,ratifyDate,validityDate,contactPeroson,tel,cccBm,cccName) values
	     (#{id},#{instType},#{instTypeOrder},#{ratifyDateOrder},#{mainInstId},#{instCode},#{instName},#{instStatus},#{address},#{districtCode},#{districtCodeName},#{principal},#{ratifyDate},#{validityDate},#{contactPeroson},#{tel},#{cccBm},#{cccName})
    </insert>

</mapper>