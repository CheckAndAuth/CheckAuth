<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.check.auth.g3.core.checkauth.dao.AuthInstMapper">

<select id="selectAuthInstByInstCode" resultType="com.check.auth.g3.core.checkauth.entity.AuthInstEntity" parameterType="java.lang.String">
    SELECT * FROM tbl_auth_Inst WHERE instCode=#{instCode}
</select>

<insert id="insertAuthInst" parameterType="com.check.auth.g3.core.checkauth.entity.AuthInstEntity">
    insert into tbl_auth_Inst
    (id,instType,instTypeOrder,ratifyDateOrder,mainInstId,instCode,instName,instStatus,address,districtCode,districtCodeName,principal,ratifyDate,validityDate,contactPeroson,tel,cccBm,cccName)
    values(#{id},#{instType},#{instTypeOrder},#{ratifyDateOrder},#{mainInstId},#{instCode},#{instName},#{instStatus},#{address},#{districtCode},#{districtCodeName},#{principal},#{ratifyDate},#{validityDate},#{contactPeroson},#{tel},#{cccBm},#{cccName})
     </insert>
</mapper>

